"Azure Resource Manager (ARM) Template": {
    "prefix": "arm!",
    "body": [
        "{",
        "    \"$$schema\": \"https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#\",",
        "    \"contentVersion\": \"1.0.0.0\",",
        "    \"parameters\": {},",
        "    \"variables\": {},",
        "    \"resources\": [],",
        "    \"outputs\": {}",
        "}"
    ],
    "description": "Skeleton ARM Template"
},
"Azure Resource Manager (ARM) Parameters Template": {
    "prefix": "armp!",
    "body": [
        "{",
        "    \"$$schema\": \"https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#\",",
        "    \"contentVersion\": \"1.0.0.0\",",
        "    \"parameters\": {}",
        "}"
    ],
    "description": "Skeleton ARM Parameters Template"
},
"Variable": {
    "prefix": "arm-variable",
    "body": [
        "\"${1:variableName}\": \"${2:variableValue}\""
    ],
    "description": "ARM Variable"
},
"Parameter": {
    "prefix": "arm-parameter",
    "body": [
        "\"${1:parameterName}\": {",
        "   \"type\": \"${2:string}\",",
        "   \"metadata\": {",
        "        \"description\": \"${3:}\"",
        "    }",
        "}"
    ],
    "description": "ARM Parameter"
},
"Output": {
    "prefix": "arm-output",
    "body": [
        "\"${1:outputName}\": {",
        "   \"type\": \"${2:outputType}\",",
        "   \"value\": \"${3:outputValue}\"",
        "}"
    ],
    "description": "Template output"
},
"Deployment Parameter Value": {
    "prefix": "arm-paramvalue",
    "body": [
        "\"${1:parameterName}\": {",
        "    \"value\": \"${2:parameterValue}\"",
        "}"
    ],
    "description": "ARM Parameter value used in a azuredeploy.parameters.json file"
},
